server:
   port: 9040
   
      
eureka:
   client:
      service-url:
         defaultZone: http://localhost:9030/eureka
         
   instance:
      preferIpAddress: true
      hostname: localhost
      
spring:
  application:
     name: api-getway
  cloud:
    gateway:
      routes:
      - id: user-service
        uri: http://localhost:9040=
        predicates:
        - Path=/user/**
      - id: contact-service
        uri: http://contact-service
        predicates:
        - Path=/contact/**